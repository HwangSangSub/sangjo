<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "sangjo.mapper.MemberMapper">
	<select id="selectMemberById" parameterType="string" resultType="sangjo.vo.MemberVO">
		select * 
		from tbl_member
		where member_id = #{memberId}
	</select>
	<select id="selectMemberByIdPw" resultType="sangjo.vo.MemberVO">
		select * 
		from tbl_member
		where member_id = #{id}
		and member_pw = #{pw}
	</select>
</mapper>